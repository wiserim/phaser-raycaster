{
  "name": "phaser-raycaster",
  "version": "0.10.10",
  "description": "Raycasting plugin for Phaser 3.",
  "main": "dist/phaser-raycaster.min.js",
  "types": "types/types.d.ts",
  "scripts": {
    "dev": "webpack --mode development --config config/webpack.config.dev.js --watch",
    "build": "webpack --mode development --config config/webpack.config.dev.js && webpack --mode production --config config/webpack.config.js",
    "build-typings": "node config/build-typings.js",
    "build-docs": "node node_modules/jsdoc/jsdoc.js -c config/jsdoc.conf.json -R README.md"
  },
  "browserslist": [
    "last 1 version",
    "> 1%"
  ],
  "keywords": [
    "phaser",
    "phaser3",
    "phaser-plugin",
    "plugin",
    "raycast"
  ],
  "author": "Marcin Walczak",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/wiserim/phaser-raycaster.git"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/register": "^7.17.7",
    "babel-loader": "^8.2.5",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.11",
    "tsd-jsdoc": "^2.5.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  }
}
